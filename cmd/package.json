    "templates",
    "starter",
    "monorepo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rankangkang/kk.git"
  },
  "author": {
    "name": "rankangkang",
    "url": "https://github.com/rankangkang"
  },
  "type": "module",
  "bin": {
    "create-kk": "./bin/index.cjs",
    "kk": "./bin/index.cjs"
  },
