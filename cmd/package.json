  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/debug": "^4.1.8",
    "@types/node": "^18.17.5",
    "@types/prompts": "^2.4.4",
    "esbuild": "^0.18.16",
    "esbuild-plugin-license": "^1.2.2",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
